{
    "name": "forecast",
    "runtime": "python3.6",
    "memory": 128,
    "timeout": 5,
    "role": "arn:aws:iam::100994446770:role/forecast_lambda_function",
    "hooks": {
        "build": "pip install -r requirements.txt -t .",
